      ],
      "@umbraco-cms/backoffice/relations": ["src/packages/relations/relations"],
      "@umbraco-cms/backoffice/tags": ["src/packages/tags"],
      "@umbraco-cms/backoffice/static-file": ["src/packages/static-file"],
      "@umbraco-cms/backoffice/partial-view": [
