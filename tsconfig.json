      ],
      "@umbraco-cms/backoffice/member-type": [
        "src/packages/members/member-types"
      ],
      "@umbraco-cms/backoffice/package": ["src/packages/packages/package"],
      "@umbraco-cms/backoffice/language": ["src/packages/language"],
      "@umbraco-cms/backoffice/dynamic-root": ["src/packages/dynamic-root"],
      "@umbraco-cms/backoffice/logviewer": ["src/packages/settings/logviewer"],
      "@umbraco-cms/backoffice/relation-type": [
