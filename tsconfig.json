      "@umbraco-cms/backoffice/collection": ["src/packages/core/collection"],
      "@umbraco-cms/backoffice/components": ["src/packages/core/components"],
      "@umbraco-cms/backoffice/content-type": [
        "src/packages/core/content-type"
      ],
      "@umbraco-cms/backoffice/culture": ["src/packages/core/culture"],
      "@umbraco-cms/backoffice/data-type": ["src/packages/core/data-type"],
