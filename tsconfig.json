      ],
      "@umbraco-cms/backoffice/stylesheet": [
        "src/packages/templating/stylesheets"
      ],
      "@umbraco-cms/backoffice/template": ["src/packages/templating/templates"],
      "@umbraco-cms/backoffice/user-group": ["src/packages/user/user-group"],
      "@umbraco-cms/backoffice/current-user": [
        "src/packages/user/current-user"
      ],
      "@umbraco-cms/backoffice/user": ["src/packages/user/user"],
